{
    "name": "celaraze/dcat-routes",
    "description": "DcatAdmin 自动生成资源路由。",
    "alias": "Dcat 资源路由",
    "type": "library",
    "keywords": [
        "dcat-admin",
        "extension"
    ],
    "homepage": "https://gitee.com/celaraze/dcat-routes",
    "license": "MIT",
    "authors": [
        {
            "name": "celaraze",
            "email": "celaraze@qq.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "dcat/laravel-admin": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Celaraze\\DcatRoutes\\": "src/"
        }
    },
    "extra": {
        "dcat-admin": "Celaraze\\DcatRoutes\\ServiceProvider",
        "laravel": {
            "providers": [
                "Celaraze\\DcatRoutes\\ServiceProvider"
            ]
        }
    }
}
